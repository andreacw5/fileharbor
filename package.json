{
  "name": "fileharbor",
  "version": "1.6.2",
  "description": "FileHarbor is a file storage service that allows you to upload, download, and delete files",
  "author": "Andrea Tombolato <hey@heyatom.dev> (https://heyatom.dev)",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "prisma:generate": "prisma generate",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@logtail/pino": "^0.5.2",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.13",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.13",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.13",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^11.1.1",
    "@prisma/client": "^6.5.0",
    "@types/multer": "^1.4.12",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "cache-manager": "^6.4.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^4.1.0",
    "express": "^4.21.2",
    "file-type-mime": "^0.4.6",
    "generate-password-ts": "^1.6.5",
    "globals": "^15.15.0",
    "joi": "^17.13.3",
    "multer": "1.4.5-lts.1",
    "nestjs-pino": "^4.4.0",
    "passport": "^0.7.0",
    "passport-headerapikey": "^1.2.2",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "prisma": "^6.5.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sharp": "^0.33.5",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^11.0.13",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__template": "^7.4.4",
    "@types/babel__traverse": "^7.20.7",
    "@types/bcrypt": "10.0.28-alpha",
    "@types/cookiejar": "^2.1.5",
    "@types/eslint-scope": "^3.7.7",
    "@types/estree": "^1.0.7",
    "@types/express": "^4.17.21",
    "@types/gensync": "^1.0.4",
    "@types/graceful-fs": "^4.1.9",
    "@types/http-errors": "^2.0.4",
    "@types/istanbul-lib-coverage": "^2.0.6",
    "@types/istanbul-lib-report": "^3.0.3",
    "@types/istanbul-reports": "^3.0.4",
    "@types/jest": "^29.5.14",
    "@types/json-schema": "^7.0.15",
    "@types/luxon": "^3.6.2",
    "@types/methods": "^1.1.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.14.0",
    "@types/send": "^0.17.4",
    "@types/stack-trace": "^0.0.33",
    "@types/stack-utils": "^2.0.3",
    "@types/superagent": "^8.1.9",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@types/yargs": "^17.0.33",
    "@types/yargs-parser": "^21.0.3",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.6",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@nestjs/core",
      "nestjs-pino"
    ],
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "bcrypt",
      "nestjs-pino"
    ]
  }
}
